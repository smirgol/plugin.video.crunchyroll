# Device Authentication Dialog

msgctxt "#30300"
msgid "Visit: %s"
msgstr "Besuchen Sie: %s"

msgctxt "#30301"
msgid "Please visit the URL above and enter the code"
msgstr "Bitte besuchen Sie die obige URL und geben Sie den Code ein"

msgctxt "#30302"
msgid "Time remaining: %02d:%02d"
msgstr "Verbleibende Zeit: %02d:%02d"

msgctxt "#30303"
msgid "New code generated - please visit the URL again"
msgstr "Neuer Code generiert - bitte besuchen Sie die URL erneut"

msgctxt "#30304"
msgid "✓ Authentication successful!"
msgstr "✓ Authentifizierung erfolgreich!"

msgctxt "#30305"
msgid "⚠ Code expired - click Refresh for new code"
msgstr "⚠ Code abgelaufen - klicken Sie auf Aktualisieren für neuen Code"

